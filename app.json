{
  "expo": {
    "name": "expo chat",
    "slug": "rn-chat",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon2.png",
    "splash": {
      "image": "./assets/splash2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.bopboy.www",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "The app accesses to photo library to upload your profile when you are signing up or updating profile."
      }
    },
    "android": {
      "package": "com.bopboy.www",
      "versionCode": 1,
      "permissions": [
        "CAMERA"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon2.png",
        "backgroundColor": "#4294F7"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}